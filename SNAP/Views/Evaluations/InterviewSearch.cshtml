@model IEnumerable<SNAP.Models.Persistent.CandidateInterview>

@{
    ViewBag.Title = "Interview Search";
}

<h2>@ViewBag.Title</h2>

<hr />

@using (Html.BeginForm("InterviewSearch", "Evaluations", FormMethod.Get))
{
    <div class="form-inline">
        <div class="form-group">
            @Html.Editor("CandidateName", new { htmlAttributes = new { @class = "form-control keyup-submit", placeholder = "Candidate Name" } })
        </div>
        <div class="form-group">
            @Html.DropDownList("ReqTitle", new SelectList(ViewBag.Titles), "Requisition Title", new { @class = "form-control change-submit", placeholder = "Requisition Title" })
        </div>
        <div class="form-group">
            @Html.DropDownList("ReqNumber", new SelectList(ViewBag.Numbers), "Requisition Number", new { @class = "form-control change-submit", placeholder = "Requisition Number" })
        </div>
    </div>
}

<br />

<div id="ajaxPartial">
    @Html.Partial("_InterviewSearchPartial", Model)
</div>