@model IEnumerable<SNAP.Models.Persistent.Contingent>

@{
    ViewBag.Title = "Contingent Search";
}

<h2>@ViewBag.Title</h2>

<hr />

@using (Html.BeginForm("Index", "Contingents", FormMethod.Get))
{
    <div class="form-inline">
        <div class="form-group">
            @Html.Editor("FirstName", new { htmlAttributes = new { @class = "form-control keyup-submit", placeholder = "First Name" } })
        </div>
        <div class="form-group">
            @Html.Editor("LastName", new { htmlAttributes = new { @class = "form-control keyup-submit", placeholder = "Last Name" } })
        </div>
    </div>
}

<br />

<div id="ajaxPartial">
    @Html.Partial("_IndexPartial", Model)
</div>