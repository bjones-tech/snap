@model IEnumerable<SNAP.Models.Persistent.GSCUser>

@{
    ViewBag.Title = "GSC Users";
    ViewBag.SubTitle = "Previously Separated";
}

<h2>@ViewBag.Title <small class="text-danger">@ViewBag.SubTitle</small></h2>

<br />

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GSCClient.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GSCClient.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
    </tr>
}

</table>
