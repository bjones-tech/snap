@model IEnumerable<SNAP.Models.Persistent.GSCUser>

@{
    ViewBag.Title = "GSC Users";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Add User", "Create", null, new { @class = "btn btn-default" })
</p>

<hr />

@using (Html.BeginForm("Index", "GSCUsers", FormMethod.Get))
{
    <div class="form-inline">
        <div class="form-group">
            @Html.Editor("Name", new { htmlAttributes = new { @class = "form-control keyup-submit", placeholder = "Name" } })
        </div>
        <div class="form-group">
            @Html.DropDownList("Client", new SelectList(ViewBag.Clients), "Client", new { @class = "form-control change-submit", placeholder = "Client" })
        </div>
    </div>
}

<br />

<div id="ajaxPartial">
    @Html.Partial("_IndexPartial", Model)
</div>