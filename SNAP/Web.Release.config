<?xml version="1.0"?>

<!-- For more information on using Web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=301874 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="SNAP_URI" value="https://localhost" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="NAVBAR_COLOR" value="background-color: #455565" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
  <system.net xdt:Transform="Replace">
    <mailSettings>
      <smtp deliveryMethod="Network" from="AM.SNAP.Notifications@dimensiondata.com">
        <network host="mx2.us.didata.com" />
      </smtp>
    </mailSettings>
  </system.net>
</configuration>